<Application x:Class="Snake.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
	<Application.Resources>
		<SolidColorBrush x:Key="BackgroundColor" Color="White" />
		<SolidColorBrush x:Key="TextColor" Color="Black" />
		<FontFamily x:Key="MainFont">Arial</FontFamily>
		<SolidColorBrush x:Key="GridLineColor" Color="Gray" />
		<SolidColorBrush x:Key="GridBackgroundColor" Color="LightGray" />
	</Application.Resources>
</Application>